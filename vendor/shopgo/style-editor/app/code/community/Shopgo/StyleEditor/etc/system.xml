<?xml version="1.0"?>
<config>
    <tabs>
        <shopgo translate="label">
            <label>Shopgo Extensions</label>
            <sort_order>0</sort_order>
        </shopgo>
    </tabs>
    <sections>
        <styleeditor translate="label" module="styleeditor">
            <class>separator-top</class>
            <label>Style Editor</label>
            <tab>shopgo</tab>
            <frontend_type>text</frontend_type>
            <sort_order>300</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <general translate="label">
                    <label>Stylesheet Editor</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>0</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <!-- <active translate="label">
                            <label>Enable Module</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>0</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </active> -->
                        <styleeditor translate="label comment">
                            <label>ShopGo StyleEditor</label>
                            <frontend_type>button</frontend_type>
                            <frontend_model>styleeditor/adminhtml_system_config_styleeditor_button</frontend_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <!-- <comment></comment> -->
                        </styleeditor>
                        <version translate="label">
                            <label>StyleEditor version</label>
                            <frontend_type>select</frontend_type>
                            <frontend_model>styleeditor/adminhtml_system_config_styleeditor_version</frontend_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </version>
                    </fields>
                </general>
                <logo translate="label">
                    <label>Logo</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <position translate="label comment">
                            <label>Logo Position</label>
                            <frontend_type>radios</frontend_type>
                            <!--<frontend_model>styleeditor/adminhtml_system_config_styleeditor_logo</frontend_model>-->
                            <source_model>styleeditor/logo</source_model>
                            <!--<backend_model>styleeditor/system_config_backend_serialized</backend_model>-->
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </position>
                        <layout translate="label comment">
                            <label></label>
                            <frontend_type>select</frontend_type>
                            <frontend_model>styleeditor/adminhtml_system_config_styleeditor_logolayout</frontend_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </layout>
                    </fields>
                </logo>
                <slider translate="label">
                    <label>Slider</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>2</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <position translate="label comment">
                            <label>Slider Style</label>
                            <frontend_type>radios</frontend_type>
                            <!--<frontend_model>styleeditor/adminhtml_system_config_styleeditor_logo</frontend_model>-->
                            <source_model>styleeditor/slider</source_model>
                            <!--<backend_model>styleeditor/system_config_backend_serialized</backend_model>-->
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </position>
                        <layout translate="label comment">
                            <label></label>
                            <frontend_type>select</frontend_type>
                            <frontend_model>styleeditor/adminhtml_system_config_styleeditor_sliderlayout</frontend_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </layout>
                        <block translate="label comment">
                            <label>Static Block</label>
                            <frontend_type>select</frontend_type>
                            <!--<frontend_model>styleeditor/adminhtml_system_config_styleeditor_logo</frontend_model>-->
                            <source_model>styleeditor/block</source_model>
                            <!--<backend_model>styleeditor/system_config_backend_serialized</backend_model>-->
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </block>
                    </fields>
                </slider>
                <flippers translate="label">
                    <label>Images Flippers</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>4</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <count translate="label comment">
                            <label>Images Count</label>
                            <frontend_type>radios</frontend_type>
                            <!--<frontend_model>styleeditor/adminhtml_system_config_styleeditor_logo</frontend_model>-->
                            <source_model>styleeditor/flipperscount</source_model>
                            <!--<backend_model>styleeditor/system_config_backend_serialized</backend_model>-->
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </count>
                        <layout translate="label comment">
                            <label></label>
                            <frontend_type>select</frontend_type>
                            <frontend_model>styleeditor/adminhtml_system_config_styleeditor_flipperslayout</frontend_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </layout>
                        <speed translate="label comment">
                            <label>Speed</label>
                            <frontend_type>text</frontend_type>
                            <source_model>styleeditor/flippersspeed</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </speed>
                    </fields>
                </flippers>
                <brands translate="label">
                    <label>Brands</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <position translate="label comment">
                            <label>Position</label>
                            <frontend_type>radios</frontend_type>
                            <!--<frontend_model>styleeditor/adminhtml_system_config_styleeditor_logo</frontend_model>-->
                            <source_model>styleeditor/brands</source_model>
                            <!--<backend_model>styleeditor/system_config_backend_serialized</backend_model>-->
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </position>
                        <layout translate="label comment">
                            <label></label>
                            <frontend_type>select</frontend_type>
                            <frontend_model>styleeditor/adminhtml_system_config_styleeditor_brandslayout</frontend_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <!-- <comment></comment> -->
                        </layout>
                    </fields>
                </brands>


            </groups>
        </styleeditor>
    </sections>
</config>
