<?xml version="1.0"?>
<config>
	<modules>
		<Shopgo_Store>
			<version>1.0.2</version>
		</Shopgo_Store>
		<translations>
			<version>1.0.0</version>
		</translations>
	</modules>
	<global>
		<helpers>
			<store>
				<class>Shopgo_Store_Helper</class>
			</store>
		</helpers>
		<blocks>
			<store>
				<class>Shopgo_Store_Block</class>
			</store>
		</blocks>
		<models>
			<store>
				<class>Shopgo_Store_Model</class>
			</store>
		</models>
	</global>

	<frontend>
		<translate>
			<modules>
				<translations>
					<files>
						<default>Shopgo_Store.csv</default>
					</files>
				</translations>
			</modules>
		</translate>
		<routers>
			<store>
				<use>standard</use>
				<args>
					<module>Shopgo_Store</module>
					<frontName>store</frontName>
				</args>
			</store>
		</routers>
	</frontend>
	<adminhtml>
		<translate>
			<modules>
				<translations>
					<files>
						<default>Shopgo_Store.csv</default>
					</files>
				</translations>
			</modules>
		</translate>
	</adminhtml>
	<admin>
		<routers>
			<adminhtml>
				<args>
                    <modules>
                        <store after="Mage_Adminhtml">Shopgo_Store_Adminhtml</store>
                    </modules>
				</args>
			</adminhtml>
			<!--<store>-->
				<!--<use>admin</use>-->
				<!--<args>-->
					<!--<module>Shopgo_Store</module>-->
					<!--<frontName>Store</frontName>-->
				<!--</args>-->
			<!--</store>-->
		</routers>
	</admin>
</config>
