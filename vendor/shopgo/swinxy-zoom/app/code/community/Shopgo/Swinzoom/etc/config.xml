<?xml version="1.0"?>
<config>
    <modules>
        <Shopgo_Swinzoom>
            <version>0.0.1</version>
        </Shopgo_Swinzoom>
    </modules>
    <frontend>
        <routers>
            <swinzoom>
                <use>standard</use>
                <args>
                    <module>Shopgo_Swinzoom</module>
                    <frontName>swinzoom</frontName>
                </args>
            </swinzoom>
        </routers>
        <layout>
            <updates>
                <swinzoom>
                    <file>shopgo/swinzoom.xml</file>
                </swinzoom>
            </updates>
        </layout>
    </frontend>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <swinzoom after="Mage_Adminhtml">Shopgo_Swinzoom_Adminhtml</swinzoom>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
		<layout>
			<updates>
				<swinzoom>
					<file>shopgo/swinzoom.xml</file>
				</swinzoom>
