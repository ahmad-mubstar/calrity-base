    <global>
        <models>
            <swinzoom>
                <class>Shopgo_Swinzoom_Model</class>
                <resourceModel>swinzoom_mysql4</resourceModel>
            </swinzoom>
            <swinzoom_mysql4>
                <class>Shopgo_Swinzoom_Model_Mysql4</class>
                <entities>
                    <swinzoom>
                        <table>swinzoom</table>
                    </swinzoom>
                </entities>
            </swinzoom_mysql4>
            <!-- <customer>
		        <rewrite>
		            <attribute>Shopgo_Swinzoom_Model_Address_Attribute</attribute>
		        </rewrite>
	        </customer> -->
        </models>
        <resources>
            <swinzoom_setup>
                <setup>
                    <module>Shopgo_Swinzoom</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </swinzoom_setup>
            <swinzoom_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </swinzoom_write>
            <swinzoom_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </swinzoom_read>
        </resources>
        <blocks>
            <swinzoom>
                <class>Shopgo_Swinzoom_Block</class>
            </swinzoom>
        </blocks>
        <helpers>
            <swinzoom>
                <class>Shopgo_Swinzoom_Helper</class>
            </swinzoom>
        </helpers>
    </global>
		<default>
			<shopgo_swinzoom>
				<general>
					<jquery>0</jquery>
				</general>
			</shopgo_swinzoom>
		</default>
</config>