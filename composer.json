    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://magento1.satis.shopgo.io",
            "options": {
                "ssh2": {
                    "username": "composer",
                    "pubkey_file": "~/.ssh/satis/packages.pub",
                    "privkey_file": "~/.ssh/satis/packages"
                }
            }
        }
    ],
    "extra": {
        "magento-root-dir": "./",
        "magento-core-deploy" : {
            "excludes": [
                "composer.json"
            ]
